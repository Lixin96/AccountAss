<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="T_Account" schema="dbo" catalog="AccountDB"
		class="Model.TAccount">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_AccountType" schema="dbo" catalog="AccountDB"
		class="Model.TAccountType">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="T_User" schema="dbo" catalog="AccountDB"
		class="Model.TUser">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
<table name="V_Account" schema="dbo" catalog="AccountDB"
	class="Model.VAccount">
	<primary-key>
		<generator class="identity"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>